<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ID:"
            android:textColor="#000000"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_diachi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_admin_nhaphanphoi_id"
            android:text="Địa chỉ:"
            android:textColor="#000000"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_admin_nhaphanphoi_diachi"
            android:text="Email:"
            android:textColor="#000000"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_ngay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_admin_nhaphanphoi_email"
            android:text="Ngày:"
            android:textColor="#000000"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_soluong"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_admin_nhaphanphoi_sdt"
            android:layout_marginTop="0dp"
            android:text="Số lượng:"
            android:textColor="#000000"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_sdt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_admin_nhaphanphoi_ngay"
            android:layout_marginTop="1dp"
            android:text="Số điện thoại: "
            android:textColor="#000000"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_tensp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_admin_nhaphanphoi_soluong"
            android:layout_marginTop="2dp"
            android:text="Tên sản phẩm:"
            android:textColor="#000000"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_admin_nhaphanphoi_tennpp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_admin_nhaphanphoi_tensp"
            android:layout_marginTop="2dp"
            android:text="Tên nhà phân phối:"
            android:textColor="#000000"
            android:textSize="20dp" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>